{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    },
    "target": "ES6",
    "module": "ESNext",
    "allowSyntheticDefaultImports": true
  },
  "vueCompilerOptions": {
    "target": 2.7
  },
  "allowJs": true,
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
